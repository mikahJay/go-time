VITE_GOOGLE_CLIENT_ID=
VITE_GOOGLE_CLIENT_SECRET=

VITE_APPLE_CLIENT_ID=
VITE_APPLE_TEAM_ID=
VITE_APPLE_KEY_ID=
# For Apple, you may store the private key text or a path; keep it secure
VITE_APPLE_PRIVATE_KEY=

VITE_FACEBOOK_APP_ID=
VITE_FACEBOOK_APP_SECRET=

# OAuth redirect URI used by your providers (example)
VITE_OAUTH_REDIRECT_URI=http://localhost:5173/auth/callback

# Notes:
# - In Vite, env variables that should be exposed to the client must be prefixed with VITE_.
# - Keep real secrets out of client builds; prefer sending server-side tokens from a backend.
