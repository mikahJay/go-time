# RESOURCE_STORE: 'memory' (default) or 'dynamo'
RESOURCE_STORE=dynamo
AWS_SDK_LOAD_CONFIG=1
# DynamoDB settings (only used when RESOURCE_STORE=dynamo)
AWS_REGION=us-east-2
DYNAMO_TABLE=Resources

# Optional: point at LocalStack or a custom DynamoDB endpoint
# DYNAMO_ENDPOINT=http://localhost:4566

# Optional: use a named AWS profile (the SDK will pick up shared credentials)
AWS_PROFILE=dev
